extensions:
    ZaxCMSArticle: ZaxCMS\DI\ArticleExtension

## NULL = show only if can edit
ZaxCMSArticle:
    list:
        itemsPerPage: 10
    category:
        showBreadCrumb: NULL
        showSubcategories: NULL
    article:
        showTimePosted: FALSE
        showAuthor: FALSE
        showCategory: FALSE
        showTags: FALSE
        tagsOnBottom: FALSE
        defaults:
            isVisibleInRootCategory: TRUE
            isMain: FALSE
            isPublic: FALSE